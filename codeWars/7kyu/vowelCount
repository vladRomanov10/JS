function getCount(str) {
    const vowels = ['a','e','i','o','u'];
    let countOfVowels = 0;
    for (let i = 0; i < str.length; i++) {
        for (let z = 0; z < vowels.length; z++) {
            if (str[i] === vowels[z]) {
                countOfVowels++;
            }
        }
    }
    return countOfVowels;
}